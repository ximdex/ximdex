{
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "codemirror/Codemirror",
                "version": "4.7.0",
                "source": {
                    "url": "https://github.com/codemirror/CodeMirror.git",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "RyanMullins/angular-hammer",
                "version": "2.1.10",
                "source": {
                    "url": "https://github.com/RyanMullins/angular-hammer.git",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "hammerjs/hammer.js",
                "version": "2.0.4",
                "source": {
                    "url": "https://github.com/hammerjs/hammer.js.git",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        }
    ],
    "require": {
        "solarium/solarium": "3.5.1",
        "mnapoli/php-di": "5.1.0",
        "monolog/monolog": "1.11.0",
        "codemirror/Codemirror": "4.7.0",
        "RyanMullins/angular-hammer": "2.1.10",
        "hammerjs/hammer.js": "2.0.4",
        "werkint/jsmin": "dev-master",
        "pda/pheanstalk": "*",
        "symfony/console": "^3.1",
        "symfony/polyfill-php56": "^1.2",
        "php-di/php-di": "^5.4",
        "doctrine/cache": "^1.6",
        "doctrine/common": "^2.7",
        "laravel/lumen-framework": "^5.4"
    },
     "require-dev": {
        "mockery/mockery": "0.8.0",
        "codeception/codeception": "^2.2",
        "symfony/var-dumper": "^3.2"
    },
    "autoload": {
        "psr-0": { "Ximdex": "lib"}
    },
    "config": {
        "vendor-dir": "extensions/vendors/",
                    "secure-http": false
    }
}
