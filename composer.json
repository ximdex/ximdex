        "cms",
        "semantic"
    ],
    "homepage": "https://github.com/XIMDEX/ximdex",
    "repositories": [],
    "require": {
        "php": ">=7.1",
        "solarium/solarium": "3.8.*",
        "monolog/monolog": "1.23.*",
        "pda/pheanstalk": "3.1.*",
        "symfony/console": "3.3.*",
        "php-di/php-di": "5.4.*",
        "doctrine/common": "2.8.*",
        "illuminate/support": "5.5.*",
        "phpseclib/phpseclib": "2.0.*",
        "phpmailer/phpmailer": "^6.0",
        "kevinlebrun/colors.php": "1.*"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "codeception/codeception": "2.3.*"
    },
    "autoload": {
        "psr-4": {
            "Ximdex\\": "src",
            "XimdexApi\\": "api/src"
        }
    },
    "config": {
        "vendor-dir": "vendors/"
